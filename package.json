    "go-cqwebsocket": "^6.2.2",
    "image_searcher": "^0.0.3",
    "jimp": "^0.16.2",
    "jsonc": "^2.0.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "node-emoji": "^1.11.0",
    "node-fetch": "^3.3.0",
    "nodemon": "^2.0.20",
